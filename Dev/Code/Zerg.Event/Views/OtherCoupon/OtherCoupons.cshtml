@{
    ViewBag.Title = "Coupons";
    Layout = null;
}
@model dynamic
<link href="~/Controllers/Coupons/Static/style/Coupons.css" rel="stylesheet" />
<link href="~/Controllers/Coupons/Static/style/common.css" rel="stylesheet" />
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<div class="window">
    <div class="hd">
        <div class="header cf">
            <div class="logo fl"><a href="#">创富宝</a> </div>
            <div class="search fr">
                <label for="houses">456</label><input name="" id="houses" type="text" maxlength="10" placeholder="搜索楼盘" ng-model="Condition" />
                <!--<button ng-click="getBrandList()">搜索</button>-->
                <a style="width: 80px;border: none;font-size: 24px;color: #ffffff;background: none;" href="#/app/brand?condition={{Condition}}">搜索</a>
            </div>
        </div>
    </div>
    <div class="Coupons-content" style="background-color: #ebebeb;width: 640px">

        <div class="saveMoney">距离本场结束：&nbsp;03:47:32</div>
        <div class="couponsList">
            <ul>
                @foreach (var item in Model)
                {
                    <li class="distance">
                        <img src="http://img.yoopoon.com/@item.BrandImg" width="340px" height="222px" alt="抢起来" />
                        <div class="lpIntro">
                            <span>@item.ProductParamater["最高优惠"]元/m²&nbsp;&nbsp;@item.ProductParamater["占地面积"]</span>
                            <p>@item.SubTitle</p>
                            <a href="http://www.iyookee.cn/#/app/housesBuy?BrandId=@item.BrandId">了解楼盘详情&nbsp;&nbsp;</a>
                        </div>
                        <div style="background-color: #ffdfa5; width: 280px; height: 222px; float: right;margin-right: 10px">
                            <div class="juan">@item.Name</div>
                            <div class="specialUse">@item.ReMark</div>
                            @*@Html.ActionLink("抢起来", "couponOwn", new { id = item.Id })*@
                            @if (@item.Count == 0)
                            {
                                <a id="coupon" href="#"><button disabled="disabled" style="background-color:#faebd7">抢起来</button></a>
                            }
                            else
                            {
                                <a id="coupon" href="/Coupons/couponOwn?id=@item.Id"><button>抢起来</button></a>
                            }

                            <small>还剩 @item.Count 张劵</small>
                        </div>
                    </li>
                }
