@model dynamic
@{
    ViewBag.Title = "couponOwn";
}
<meta name="viewport" content="target-densitydpi=device-dpi,width=640,user-scalable=no">
<link href="~/Controllers/Coupons/Static/style/Coupons.css" rel="stylesheet" />
<link href="~/Controllers/Coupons/Static/style/common.css" rel="stylesheet" />
<link href="~/Content/bootstrap.css" rel="stylesheet" />
<div class="CouponsOwn-content">
    <div class="successOwn">
        <div class="Congratulations">恭喜，您已成功领取</div>
        <div class="juan">@Model.Name</div>
        <div class="specialUse">@Model.BrandName &nbsp;专用抵用券</div>
        @*<div class="juan">7700元劵</div>
            <div class="specialUse">中天城&nbsp;·&nbsp;天阔专用抵用劵</div>*@
    </div>
    <div class="Verification-code">
        @*<div class="figure">764&nbsp;188&nbsp;812</div>*@
        <div class="figure">@Model.Number</div>
        <p>使用验证码</p>
    </div>

    <div class="Carrying">
        <p>请携带使用验证码<br />于&nbsp;<span>2015-6-18</span>&nbsp;之前<br />莅临<span>@Model.BrandName 售楼部</span>使用</p>
    </div>
    <div class="lookOver"><a href="#">查看项目详情</a><a href="#">在线导航</a></div>
</div>


