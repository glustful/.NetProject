package com.yoopoon.home.data.json;

import android.os.AsyncTask;

public class Serializer extends AsyncTask<Void, Void, Boolean> {

    public interface SerializeListener {
        public void onComplete(Boolean serializeResult);
        public boolean onSerialize();
    }

    private final SerializeListener mParseListener;
    

    protected Serializer(SerializeListener parseListener) {
        mParseListener = parseListener;
        
    }

    @Override
    protected Boolean doInBackground(Void... params) {
        

        return mParseListener.onSerialize();
    }

    @Override
    protected void onPostExecute(Boolean parseResult) {
        mParseListener.onComplete(parseResult);
    }

   
}
